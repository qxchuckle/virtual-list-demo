  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-legacy": "^5.3.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "npm-run-all2": "^6.1.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.74.1",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.8",

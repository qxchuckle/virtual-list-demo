{
  "name": "qx-vue-virtual-list",
  "version": "1.0.0",
  "description": "Vue虚拟列表组件",
  "private": false,
  "type": "module",
  "main": "./dist/qx-virtual-list.umd.cjs",
  "module": "./dist/qx-virtual-list.js",
  "exports": {
    ".": {
      "import": "./dist/qx-virtual-list.js",
      "require": "./dist/qx-virtual-list.umd.cjs"
